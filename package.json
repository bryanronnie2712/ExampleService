{
  "name": "brymeet-desktop",
  "version": "1.0.0",
  "main": "app.js",
  "description": "A minimal Electron-React-Express video conferencing application",
  "scripts": {
    "test": "tape spec.js",
    "start": "node app.js"
  },
  "repository": "https://github.com/bryanronnie2712/BryMeet-Desktop",
  "keywords": [
    "Electron",
    "React",
    "Express",
    "Socket.io",
    "Video Conferencing"
  ],
  "author": "Bryan Ronnie",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.1.4",
    "babel-loader": "8.1.0",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "^4.4.1",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "6.3.2",
    "cors": "^2.8.5",
    "css-loader": "3.6.0",
    "electron": "9.3.1",
    "electron-packager": "^15.2.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "^4.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "nodemon": "^2.0.2",
    "reactstrap": "^8.4.1",
    "rimraf": "3.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "standard": "^14.3.3",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.12",
    "xss": "^1.0.8"
  },
  "dependencies": {
    "express": "^4.15.4",
    "simple-fetch": "^1.5.0",
    "supertest": "^3.0.0",
    "tape": "^4.8.0",
    "axios": "^0.21.0",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "morgan": "^1.10.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.4",
    "webpack": "4.42.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      },
      "postcss-nesting": {}
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
